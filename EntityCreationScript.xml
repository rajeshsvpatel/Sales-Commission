    <!-- 3. Sales Commission Audit Trail Entity -->
    <Entity>
        <LogicalName>crmgp_salescommission_audit_trail</LogicalName>
        <DisplayName>Sales Commission Audit Trail</DisplayName>
        <PluralName>Sales Commission Audit Trails</PluralName>
        <OwnershipType>Organization</OwnershipType>

        <Attributes>
            <Attribute>
                <LogicalName>crmgp_salescommission_audit_trailid</LogicalName>
                <DisplayName>Audit Trail ID</DisplayName>
                <Type>uniqueidentifier</Type>
                <IsPrimaryId>true</IsPrimaryId>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_name</LogicalName>
                <DisplayName>Name</DisplayName>
                <Type>string</Type>
                <MaxLength>100</MaxLength>
                <IsPrimaryName>true</IsPrimaryName>
                <AutoNumberFormat>AUDIT-{SEQNUM:5}</AutoNumberFormat>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_salesorder</LogicalName>
                <DisplayName>Sales Order</DisplayName>
                <Type>lookup</Type>
                <Target>salesorder</Target>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_changedby</LogicalName>
                <DisplayName>Changed By</DisplayName>
                <Type>lookup</Type>
                <Target>systemuser</Target>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_changedate</LogicalName>
                <DisplayName>Change Date</DisplayName>
                <Type>datetime</Type>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_fieldname</LogicalName>
                <DisplayName>Field Name</DisplayName>
                <Type>string</Type>
                <MaxLength>100</MaxLength>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_oldterritory</LogicalName>
                <DisplayName>Old Territory</DisplayName>
                <Type>string</Type>
                <MaxLength>50</MaxLength>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_olduser</LogicalName>
                <DisplayName>Old User</DisplayName>
                <Type>string</Type>
                <MaxLength>200</MaxLength>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_newterritory</LogicalName>
                <DisplayName>New Territory</DisplayName>
                <Type>string</Type>
                <MaxLength>50</MaxLength>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_newuser</LogicalName>
                <DisplayName>New User</DisplayName>
                <Type>string</Type>
                <MaxLength>200</MaxLength>
            </Attribute>
        </Attributes>
    </Entity>

</EntityDefinitions>
