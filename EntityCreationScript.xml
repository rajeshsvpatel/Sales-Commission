                <Type>boolean</Type>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_sedestoverride</LogicalName>
                <DisplayName>SE Dest Override</DisplayName>
                <Type>boolean</Type>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_sespec2override</LogicalName>
                <DisplayName>SE Spec2 Override</DisplayName>
                <Type>boolean</Type>
            </Attribute>

            <Attribute>
                <LogicalName>crmgp_sesell2override</LogicalName>
                <DisplayName>SE Sell2 Override</DisplayName>
                <Type>boolean</Type>
