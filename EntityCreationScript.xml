<?xml version="1.0" encoding="utf-8"?>
<!--
    This is a reference XML showing the structure of entities needed.
    Use this as a guide when creating entities in Dynamics 365.

    IMPORTANT: This is NOT a direct import file.
    Use the Deployment Guide for step-by-step entity creation instructions.
-->
<EntityDefinitions>

    <!-- 1. ZIP Code Entity -->
    <Entity>
        <LogicalName>new_zipcode</LogicalName>
        <DisplayName>ZIP Code</DisplayName>
        <PluralName>ZIP Codes</PluralName>
        <OwnershipType>Organization</OwnershipType>

        <Attributes>
            <Attribute>
                <LogicalName>new_zipcodeid</LogicalName>
                <DisplayName>ZIP Code ID</DisplayName>
                <Type>uniqueidentifier</Type>
                <IsPrimaryId>true</IsPrimaryId>
            </Attribute>

            <Attribute>
                <LogicalName>new_name</LogicalName>
                <DisplayName>Name</DisplayName>
                <Type>string</Type>
                <MaxLength>100</MaxLength>
                <IsPrimaryName>true</IsPrimaryName>
            </Attribute>

            <Attribute>
                <LogicalName>new_zipcode</LogicalName>
                <DisplayName>ZIP Code</DisplayName>
                <Type>string</Type>
                <MaxLength>100</MaxLength>
                <Description>ZIP code value</Description>
            </Attribute>

            <Attribute>
                <LogicalName>new_territory</LogicalName>
                <DisplayName>Territory</DisplayName>
                <Type>string</Type>
                <MaxLength>50</MaxLength>
                <Description>Territory number</Description>
            </Attribute>

            <Attribute>
                <LogicalName>new_category</LogicalName>
                <DisplayName>Category</DisplayName>
                <Type>picklist</Type>
                <OptionSet>
                    <Option value="1" label="SE"/>
                    <Option value="2" label="BC"/>
                    <Option value="3" label="SWC"/>
                </OptionSet>
            </Attribute>

            <Attribute>
                <LogicalName>new_userid</LogicalName>
                <DisplayName>User</DisplayName>
                <Type>lookup</Type>
                <Target>systemuser</Target>
                <Description>Associated system user</Description>
            </Attribute>
        </Attributes>
    </Entity>

    <!-- 2. Sales Commission Entity -->
    <Entity>
        <LogicalName>crmgp_salescommission</LogicalName>
